# GitHub OAuth 配置
# 在 https://github.com/settings/developers 创建 OAuth 应用
GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here

# Umami 网站分析配置
# 在 Umami 控制台获取这些信息
UMAMI_WEBSITE_ID=your_umami_website_id_here
UMAMI_API_TOKEN=your_umami_api_token_here
UMAMI_API_URL=https://umami.lacs.cc

# 服务器配置
PORT=3000
NODE_ENV=development 